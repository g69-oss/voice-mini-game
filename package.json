{
  "name": "nextjs-bun-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next",
    "dev": "next dev --turbo",
    "format": "prettier --log-level warn --write --cache .",
    "format:check": "prettier --log-level warn --check .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "preview": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false && next lint && next build && next start",
    "start": "next start",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false",
    "check": "bun run typecheck && bun run lint:strict && bun run format:check",
    "fix": "bun run lint:fix && bun run format"
  },
  "dependencies": {
    "microsoft-cognitiveservices-speech-sdk": "^1.38.0",
    "class-variance-authority": "^0.7.1",
    "elevenlabs": "^0.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^12.23.16",
    "lucide-react": "^0.544.0",
    "next": "^14.0.0",
    "openai": "^4.52.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.1.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0"
  },
  "packageManager": "bun@1.0.0"
}
